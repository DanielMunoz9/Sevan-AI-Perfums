{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "memory": 2048,
      "maxDuration": 30
    },
    "src/pages/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "memory": 2048,
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
